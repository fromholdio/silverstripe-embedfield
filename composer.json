{
	"name": "fromholdio/silverstripe-embedfield",
	"description": "A form field for adding oembed objects to pages or dataobjects",
	"type": "silverstripe-vendormodule",
	"homepage": "https://github.com/fromholdio/silverstripe-embedfield",
	"keywords": ["silverstripe", "oembed", "video", "cms", "formfield"],
	"license": "BSD-3-Clause",
	"authors": [
		{"name": "Luke Fromhold","email": "luke@fromhold.io"},
		{"name": "Nathan Cox","email": "me@nathan.net.nz"}
	],
	"support": {
		"issues": "https://github.com/fromholdio/silverstripe-embedfield/issues"
	},
	"require": {
		"silverstripe/framework": "~5.0",
		"embed/embed": "^4.0.0"
	},
	"autoload": {
		"psr-4": {
			"Fromholdio\\OEmbedField\\": "src/"
		}
	},
    "extra": {
        "expose": [
            "client/css",
            "client/js"
        ]
    }
}
